{
  "compilerOptions": {
    "target": "ES2020",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "skipLibCheck": true,
    "allowImportingTsExtensions": false,
    "moduleDetection": "force",
    "noEmit": false,  // Change from true to false
    "outDir": "./dist", // Add an output directory for the compiled files
    "strict": true
  },
  "include": ["src"]
}
